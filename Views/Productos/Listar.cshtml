@model List<TiendaNamespace.Producto>
<h2 class="text-center">Listado de productos</h2>
<table class = "table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Descripcion</th>
            <th scope="col">Precio</th>
            <th scope="col">-</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var p in Model){
            <tr>
                <th scope="row">@p.IdProducto</th>
                <td>@p.Descripcion</td>
                <td>@p.Precio</td>
                <td>lol</td>
            </tr>
        }
    </tbody>
</table>

